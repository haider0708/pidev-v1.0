    <div class="row justify-content-center">
        <div class="col-md-6">
            {{ form_start(form) }}
            <table class="table">
                <tr>
                    <td>{{ form_label(form.nomcategorie, 'Nom') }}</td>
                    <td>{{ form_widget(form.nomcategorie, {'attr': {'class': 'form-control'}}) }}</td>
                </tr>
            
            </table>
            <button type="submit" class="btn btn-success rounded-pill">edit</button> {# Utilisation de btn-success pour la couleur verte et rounded-pill pour la forme ronde #}
